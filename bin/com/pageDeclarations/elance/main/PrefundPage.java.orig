	 public void openPrefundPageByUrl(String bidId) {
		try {
		 String preFund = new String(resources.getString("pageURL"));
		 System.out.println(preFund);
		 String[] tmp = preFund.split("elance.com");
		 String tmpURL = tmp[1]+"#bidId&conf_mode=terms_employer";
		 preFund = tmpURL.replace("#bidId", bidId);
		 System.out.println(preFund);
		 driver.navigate().to(preFund);
		 waitForElementPresent(fundButton, 90);
		} catch (Exception e) {
			}
	}

